    "version": "0.2.0",
    "configurations": [
        {
            "name": "Go: Launch file",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${file}",
            "env": { "DEBUG": true },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Go: Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": { "DEBUG": true },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Python: File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
    ]
}
